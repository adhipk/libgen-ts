{
  "compileOnSave": true,
  "compilerOptions": {
      "outDir": "./built",
      "allowJs": true,
      "declaration": true,
      "target": "es5",
      "module": "es2020",
      "moduleResolution":"node",
      "allowUnreachableCode": false,
      "noImplicitReturns": true,
      "noImplicitAny": true,


  },
  "exclude": [
    "./node_modules/**/*",
    "./built/**/*",
    "./test/**/*",
  ],
  "include": [
      "./**/*",
      "./lib/**/*",
  ],
  
}